  author = {Taylor, Kyla W. and Joubert, Bonnie R. and Braun, Joe M. and Dilworth, Caroline and Gennings, Chris and Hauser, Russ and Heindel, Jerry J. and Rider, Cynthia V. and Webster, Thomas F. and Carlin, Danielle J.},
  year = {2016},
