  author = {Kesselmeier, Miriam and P{\"u}tter, Carolin and Volckmar, Anna-Lena and Baurecht, Hansj{\"o}rg and Grallert, Harald and Illig, Thomas and Ismail, Khadeeja and Ollikainen, Miina and Sil{\'e}n, Yasmina and {Keski-Rahkonen}, Anna and Bulik, Cynthia M. and Collier, David A. and Zeggini, Eleftheria and Hebebrand, Johannes and Scherag, Andr{\'e} and Hinney, Anke and {GCAN and WTCCC3}},
