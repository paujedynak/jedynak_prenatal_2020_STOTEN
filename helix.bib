  keywords = {Adolescent,Adult,Child,Environmental Exposure,Female,Humans,Intelligence,Longitudinal Studies,Male,Phthalic Acids,Pregnancy,Pregnancy Trimester; Third,Prenatal Exposure Delayed Effects,Young Adult},
  language = {eng},
