@article{aurrekoetxea2013,
  title = {Determinants of Self-Reported Smoking and Misclassification during Pregnancy, and Analysis of Optimal Cut-off Points for Urinary Cotinine: A Cross-Sectional Study},
  shorttitle = {Determinants of Self-Reported Smoking and Misclassification during Pregnancy, and Analysis of Optimal Cut-off Points for Urinary Cotinine},
  author = {Aurrekoetxea, Juan J. and Murcia, Mario and Rebagliato, Marisa and L{\'o}pez, Mar{\'i}a Jos{\'e} and Castilla, Ane Miren and {Santa-Marina}, Loreto and Guxens, M{\'o}nica and {Fern{\'a}ndez-Somoano}, Ana and Espada, Mercedes and Lertxundi, Aitana and Tard{\'o}n, Adonina and Ballester, Ferran},
