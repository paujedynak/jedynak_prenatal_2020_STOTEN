@article{svendsen2016,
  title = {Differentially Methylated {{DNA}} Regions in Monozygotic Twin Pairs Discordant for Rheumatoid Arthritis: An Epigenome-Wide Study},
  shorttitle = {Differentially {{Methylated DNA Regions}} in {{Monozygotic Twin Pairs Discordant}} for {{Rheumatoid Arthritis}}},
