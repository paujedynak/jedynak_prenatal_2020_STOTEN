@article{zhao2016a,
  title = {Umbilical Cord Blood {{PBDEs}} Concentrations Are Associated with Placental {{DNA}} Methylation},
  author = {Zhao, Yan and Liu, Pengcheng and Wang, Junyong and Xiao, Xirong and Meng, Xiangzhou and Zhang, Yunhui},
  year = {2016},
  month = dec,
  volume = {97},
