@article{valle-sistac2016,
  title = {Determination of Parabens and Benzophenone-Type {{UV}} Filters in Human Placenta. {{First}} Description of the Existence of Benzyl Paraben and Benzophenone-4},
  author = {{Valle-Sistac}, Jennifer and {Molins-Delgado}, Daniel and D{\'i}az, Marta and Ib{\'a}{\~n}ez, Lourdes and Barcel{\'o}, Dami{\`a} and {Silvia D{\'i}az-Cruz}, M.},
