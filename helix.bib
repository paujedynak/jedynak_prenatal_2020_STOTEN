@article{braun_early-life_2017,
  title = {Early-Life Exposure to {{EDCs}}: Role in Childhood Obesity and Neurodevelopment},
  shorttitle = {Early-Life Exposure to {{EDCs}}},
  author = {Braun, Joseph M.},
  year = {2017},
  month = mar,
  volume = {13},
