  title = {Acetaminophen Use during Pregnancy: Effects on Risk for Congenital Abnormalities},
  shorttitle = {Acetaminophen Use during Pregnancy},
  author = {Rebordosa, Cristina and Kogevinas, Manolis and {Horv{\'a}th-Puh{\'o}}, Erzs{\'e}bet and N{\o}rg{\aa}rd, Bente and Morales, Maria and Czeizel, Andrew E. and Vilstrup, Hendrik and S{\o}rensen, Henrik T. and Olsen, J{\o}rn},
