@article{tang2020,
  title = {Oestrogenic Endocrine Disruptors in the Placenta and the Fetus},
  author = {Tang, Zi-Run and Xu, Xue-Ling and Deng, Shou-Long and Lian, Zheng-Xing and Yu, Kun},
