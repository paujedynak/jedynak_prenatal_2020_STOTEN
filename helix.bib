@article{serrati2016,
  title = {Next-Generation Sequencing: Advances and Applications in Cancer Diagnosis},
  shorttitle = {Next-Generation Sequencing},
  author = {Serrat{\`i}, Simona and De Summa, Simona and Pilato, Brunella and Petriella, Daniela and Lacalamita, Rosanna and Tommasi, Stefania and Pinto, Rosamaria},
