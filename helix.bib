  number = {6}
}

@article{almstrup2020,
  title = {Levels of Endocrine-Disrupting Chemicals Are Associated with Changes in the Peri-Pubertal Epigenome},
  author = {Almstrup, Kristian and Frederiksen, Hanne and Andersson, Anna-Maria and Juul, Anders},
  year = {2020},
  month = aug,
  volume = {9},
  pages = {845--857},
  issn = {2049-3614},
  doi = {10.1530/EC-20-0286},
  abstract = {Puberty marks a transition period, which leads to the attainment of adult sexual maturity. Timing of puberty is a strongly heritable trait. However, large genetic association studies can only explain a fraction of the observed variability and striking secular trends suggest that lifestyle and/or environmental factors are important. Using liquid-chromatography tandem-mass-spectrometry, we measured endocrine-disrupting chemicals (EDCs; triclosan, bisphenol A, benzophenone-3, 2,4-dichlorophenol, 11 metabolites from 5 phthalates) in longitudinal urine samples obtained biannually from peri-pubertal children included in the COPENHAGEN puberty cohort. EDC levels were associated with blood DNA methylation profiles from 31 boys and 20 girls measured both pre- and post-pubertally. We found little evidence of single methylation sites that on their own showed association with urinary excretion levels of EDCs obtained either the same-day or measured as the yearly mean of dichotomized EDC levels. In contrast, methylation of several promoter regions was found to be associated with two or more EDCs, overlap with known gene-chemical interactions, and form a core network with genes known to be important for puberty. Furthermore, children with the highest yearly mean of dichotomized urinary phthalate metabolite levels were associated with higher promoter methylation of the thyroid hormone receptor interactor 6 gene (TRIP6), which again was mirrored by lower circulating TRIP6 protein levels. In general, the mean TRIP6 promoter methylation was mirrored by circulating TRIP6 protein levels. Our results provide a potential molecular mode of action of how exposure to environmental chemicals may modify pubertal development.},
  file = {C\:\\Users\\pauli\\Documents\\WORK\\papers\\supplementary_figure_1.pdf;C\:\\Users\\pauli\\Documents\\WORK\\papers\\supplementary_table_1.xlsx;C\:\\Users\\pauli\\Documents\\WORK\\papers\\supplementary_table_2.xlsx;C\:\\Users\\pauli\\Zotero\\storage\\W45PY7KD\\Almstrup et al. - 2020 - Levels of endocrine-disrupting chemicals are assoc.pdf},
  journal = {Endocr Connect},
  keywords = {DNA methylation,endocrine disruption,epigenetics,phthalates,puberty},
  language = {eng},
  number = {8},
  pmcid = {PMC7487188},
  pmid = {32755991}
}

@article{altamirano2020,
  title = {Bisphenol {{A}} and Benzophenone-3 Exposure Alters Milk Protein Expression and Its Transcriptional Regulation during Functional Differentiation of the Mammary Gland in Vitro},
  author = {Altamirano, Gabriela A. and Gomez, Ayelen L. and {Schierano-Marotti}, Gonzalo and {Mu{\~n}oz-de-Toro}, M{\'o}nica and Rodriguez, Horacio A. and Kass, Laura},
  year = {2020},
  month = dec,
  volume = {191},
  pages = {110185},
  issn = {0013-9351},
  doi = {10.1016/j.envres.2020.110185},
  abstract = {The plastic monomer and plasticizer bisphenol A (BPA), and the UV-filter benzophenone-3 (BP3) have been shown to have estrogenic activities that could alter mammary gland development. Our aim was to analyze whether BPA or BP3 direct exposure affects the functional differentiation of the mammary gland using an in vitro model. Mammary organoids were obtained and isolated from 8 week-old virgin female C57BL/6 mice and were differentiated on Matrigel with medium containing lactogenic hormones and exposed to: a) vehicle (0.01\% ethanol); b) 1~\texttimes ~10-9~M or 1~\texttimes ~10-6~M BPA; or c) 1~\texttimes ~10-12~M, 1~\texttimes ~10-9~M or 1~\texttimes ~10-6~M BP3 for 72~h. The mRNA and protein expression of estrogen receptor alpha (ESR1) and progesterone receptor (PR) were assessed. In addition, mRNA levels of PR-B isoform, glucocorticoid receptor (GR), prolactin receptor (PRLR) and Stat5a, and protein expression of pStat5a/b were evaluated at 72~h. The mRNA and protein expression of milk proteins and their DNA methylation status were also analyzed. Although mRNA level of PRLR and GR was similar between treatments, mRNA expression of ESR1, total PR, PR-B and Stat5a was increased in organoids exposed to 1~\texttimes ~10-9~M BPA and 1~\texttimes ~10-12~M BP3. Total PR expression was also increased with 1~\texttimes ~10-6~M BPA. Nuclear ESR1 and PR expression was observed in all treated organoids; whereas nuclear pStat5a/b alveolar cells was observed only in organoids exposed to 1~\texttimes ~10-9~M BPA and 1~\texttimes ~10-12~M BP3. The beta-casein mRNA level was increased in both BPA concentrations and 1~\texttimes ~10-12~M BP3, which was associated with hypomethylation of its promoter. The beta-casein protein expression was only increased with 1~\texttimes ~10-9~M BPA or 1~\texttimes ~10-12~M BP3. In contrast, BPA exposure decreased alpha-lactalbumin mRNA expression and increased DNA methylation level in different methylation-sensitive sites of the gene. Also, 1~\texttimes ~10-9~M BPA decreased alpha-lactalbumin protein expression. Our results demonstrate that BPA or BP3 exposure alters milk protein synthesis and its transcriptional regulation during mammary gland differentiation in vitro.},
  file = {C\:\\Users\\pauli\\Zotero\\storage\\WFFHPKCB\\Altamirano et al. - 2020 - Bisphenol A and benzophenone-3 exposure alters mil.pdf;C\:\\Users\\pauli\\Zotero\\storage\\4GWHCBE4\\S0013935120310823.html},
  journal = {Environmental Research},
  keywords = {3D cell culture,alpha-lactalbumin,Alpha-lactalbumin,beta-casein,Beta-casein,Endocrine disruptor,mammary gland,Mammary gland,milk proteins,Milk proteins},
  language = {en},
  pmid = {32946892}
}

