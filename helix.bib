@article{schmidt_fetal_2019,
  title = {Fetal Glucocorticoid Receptor ({{Nr3c1}}) Deficiency Alters the Landscape of {{DNA}} Methylation of Murine Placenta in a Sex-Dependent Manner and Is Associated to Anxiety-like Behavior in Adulthood},
