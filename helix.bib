@article{lien2016,
  title = {Perfluoroalkyl Substances in Cord Blood and Attention Deficit/Hyperactivity Disorder Symptoms in Seven-Year-Old Children},
  author = {Lien, Guang-Wen and Huang, Ching-Chun and Shiu, Jia-Shian and Chen, Mei-Huei and Hsieh, Wu-Shiun and Guo, Yue-Liang and Chen, Pau-Chung},
  year = {2016},
