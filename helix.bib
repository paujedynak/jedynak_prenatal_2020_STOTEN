  journal = {Environ Health Perspect},
  keywords = {Adult,Checklist,Child,Child Behavior Disorders,Cohort Studies,Esters,Female,Humans,Linear Models,Logistic Models,Male,Phthalic Acids,Pregnancy,Pregnancy Trimester; Third,Prenatal Exposure Delayed Effects,Taiwan},
