@article{zhao2010,
  title = {Quantification and Application of the Placental Epigenetic Signature of the {{RASSF1A}} Gene in Maternal Plasma},
  author = {Zhao, Fuxi and Wang, Jian and Liu, Runhua and Yang, Jing and Cui, Ke and Wu, Yongming and Guo, Juncheng and Mu, Yaqin and Wang, Xiying},
