@article{batool2016,
  title = {Prenatal and Early-Life Predictors of Atopy and Allergic Disease in {{Canadian}} Children: Results of the {{Family Atherosclerosis Monitoring In earLY}} Life ({{FAMILY}}) {{Study}}},
  shorttitle = {Prenatal and Early-Life Predictors of Atopy and Allergic Disease in {{Canadian}} Children},
  author = {Batool, T. and Reece, P. L. and Schulze, K. M. and Morrison, K. M. and Atkinson, S. A. and Anand, S. S. and Teo, K. K. and Denburg, J. A. and Cyr, M. M. and {the FAMILY Study Investigators}},
  year = {2016},
