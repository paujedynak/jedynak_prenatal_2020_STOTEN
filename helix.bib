@article{cao2017a,
  title = {Exposure of Pregnant Mice to Triclosan Impairs Placental Development and Nutrient Transport},
  author = {Cao, Xinyuan and Hua, Xu and Wang, Xiaoli and Chen, Ling},
  year = {2017},
  month = mar,
  volume = {7},
