  shorttitle = {Gestational Exposure to Phthalates and Gender-Related Play Behaviors in 8-Year-Old Children},
  author = {Percy, Zana and Xu, Yingying and Sucharew, Heidi and Khoury, Jane C. and Calafat, Antonia M. and Braun, Joseph M. and Lanphear, Bruce P. and Chen, Aimin and Yolton, Kimberly},
