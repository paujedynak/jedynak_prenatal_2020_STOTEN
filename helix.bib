  author = {Alcaraz, Lindsay B. and Exposito, Jean-Yves and Chuvin, Nicolas and Pommier, Roxane M. and Cluzel, Caroline and Martel, Sylvie and Sentis, St{\'e}phanie and Bartholin, Laurent and Lethias, Claire and Valcourt, Ulrich},
