@article{goodman1999,
  ids = {goodman\_comparing\_1999},
  title = {Comparing the {{Strengths}} and {{Difficulties Questionnaire}} and the {{Child Behavior Checklist}}: Is Small Beautiful?},
  shorttitle = {Comparing the {{Strengths}} and {{Difficulties Questionnaire}} and the {{Child Behavior Checklist}}},
  author = {Goodman, R. and Scott, S.},
