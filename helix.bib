@article{braun2017b,
  title = {Prenatal Environmental Chemical Exposures and Longitudinal Patterns of Child Neurobehavior},
  author = {Braun, Joseph M. and Yolton, Kimberly and Stacy, Shaina L and Erar, Bahar and Papandonatos, George D. and Bellinger, David C. and Lanphear, Bruce P. and Chen, Aimin},
  year = {2017},
  month = sep,
  volume = {62},
  pages = {192--199},
  issn = {0161-813X},
  doi = {10.1016/j.neuro.2017.07.027},
  abstract = {Background Prenatal chemical exposures may adversely affect neurodevelopment, but few studies have examined the persistence of these associations. We examined whether associations between prenatal bisphenol A (BPA) or polybrominated diphenyl ether (PBDE) exposures persist or resolve as children age. Methods We followed 346 mother-child pairs (enrolled 2003\textendash 2006) from Cincinnati, OH from pregnancy until children were 8 years old. We measured BPA in urine collected at 16 and 26 weeks gestation and PBDE-47 in serum collected at 16 weeks gestation. We administered repeated measures of children's behavior, mental/psychomotor development, and IQ from ages 1\textendash 8 years. We determined if associations of BPA or PBDE-47 with child neurobehavior persisted or resolved as children aged using linear mixed models and estimated neurobehavioral measure reproducibility using intraclass correlation coefficients (ICCs). Results Higher BPA in girls and higher PBDE-47 in both boys and girls were associated with more externalizing behaviors; these associations persisted from ages 2\textendash 8 years (exposure \texttimes{} age interaction p-values{$\geq$}0.36). Higher PBDE-47 concentrations were associated with decreases in MDI from ages 1\textendash 3 years (PBDE-47 \texttimes{} age interaction p-value=0.03) and persistently lower IQ at ages 5 and 8 years (PBDE-47 \texttimes{} age interaction p-value=0.56). Mental/psychomotor abilities had fair reproducibility from ages 1\textendash 3 years (ICCs\textasciitilde 0.4), cognitive abilities from ages 5 to 8 years had excellent reproducibility (ICCs=0.7\textendash 0.8), and parent-reported behaviors from ages 2\textendash 8 years had poor to good reproducibility (ICCs=0.38\textendash 0.59). Conclusions Prenatal BPA and PBDE-47 concentrations were persistently associated with more externalizing behaviors. PBDE-47 concentrations were inversely associated with cognitive abilities that strengthened over time.},
  file = {C\:\\Users\\pauli\\Zotero\\storage\\QLJCPSYI\\Braun et al. - 2017 - Prenatal Environmental Chemical Exposures and Long.pdf},
  journal = {Neurotoxicology},
  pmcid = {PMC5623631},
  pmid = {28736150}
}

