@article{allison,
  title = {Measures of {{Fit}} for {{Logistic Regression}}},
  author = {Allison, Paul D},
  pages = {12},
  abstract = {One of the most common questions about logistic regression is ``How do I know if my model fits the data?'' There are many approaches to answering this question, but they generally fall into two categories: measures of predictive power (like R-square) and goodness of fit tests (like the Pearson chi-square). This presentation looks first at R-square measures, arguing that the optional R-squares reported by PROC LOGISTIC might not be optimal. Measures proposed by McFadden and Tjur appear to be more attractive. As for goodness of fit, the popular Hosmer and Lemeshow test is shown to have some serious problems. Several alternatives are considered.},
  file = {C\:\\Users\\pauli\\Zotero\\storage\\69IRAK6V\\Allison - Measures of Fit for Logistic Regression.pdf},
  language = {en}
}

@article{allmyr2006,
  title = {Triclosan in Plasma and Milk from {{Swedish}} Nursing Mothers and Their Exposure via Personal Care Products},
