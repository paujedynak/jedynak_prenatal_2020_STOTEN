  journal = {J Matern Fetal Neonatal Med},
  keywords = {Amniotic Fluid,Animals,Animals; Newborn,Aquaporin 1,Extraembryonic Membranes,Female,Fetal Development,Fetus,Homeostasis,Litter Size,Mice,Mice; Knockout,Placenta,Pregnancy},
