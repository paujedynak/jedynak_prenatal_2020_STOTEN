}

@article{santos_gestational_2018,
  title = {Gestational Weight Gain Charts for Different Body Mass Index Groups for Women in {{Europe}}, {{North America}}, and {{Oceania}}},
  author = {Santos, Susana and Eekhout, Iris and Voerman, Ellis and Gaillard, Romy and Barros, Henrique and Charles, Marie-Aline and Chatzi, Leda and Chevrier, C{\'e}cile and Chrousos, George P. and Corpeleijn, Eva and Costet, Nathalie and Crozier, Sarah and Doyon, Myriam and Eggesb{\o}, Merete and Fantini, Maria Pia and Farchi, Sara and Forastiere, Francesco and Gagliardi, Luigi and Georgiu, Vagelis and Godfrey, Keith M. and Gori, Davide and Grote, Veit and Hanke, Wojciech and {Hertz-Picciotto}, Irva and Heude, Barbara and Hivert, Marie-France and Hryhorczuk, Daniel and Huang, Rae-Chi and Inskip, Hazel and Jusko, Todd A. and Karvonen, Anne M. and Koletzko, Berthold and K{\"u}pers, Leanne K. and Lagstr{\"o}m, Hanna and Lawlor, Debbie A. and Lehmann, Irina and {Lopez-Espinosa}, Maria-Jose and Magnus, Per and Majewska, Renata and M{\"a}kel{\"a}, Johanna and Manios, Yannis and McDonald, Sheila W. and Mommers, Monique and Morgen, Camilla S. and Moschonis, George and Mur{\'i}nov{\'a}, {\v L}ubica and Newnham, John and Nohr, Ellen A. and Andersen, Anne-Marie Nybo and Oken, Emily and Oostvogels, Adri{\"e}tte J. J. M. and Pac, Agnieszka and Papadopoulou, Eleni and Pekkanen, Juha and Pizzi, Costanza and Polanska, Kinga and Porta, Daniela and Richiardi, Lorenzo and {Rifas-Shiman}, Sheryl L. and Roeleveld, Nel and {Santa-Marina}, Loreto and Santos, Ana C. and Smit, Henriette A. and S{\o}rensen, Thorkild I. A. and Standl, Marie and Stanislawski, Maggie and Stoltenberg, Camilla and Thiering, Elisabeth and Thijs, Carel and Torrent, Maties and Tough, Suzanne C. and Trnovec, Tomas and {van Gelder}, Marleen M. H. J. and {van Rossem}, Lenie and {von Berg}, Andrea and Vrijheid, Martine and Vrijkotte, Tanja G. M. and Zvinchuk, Oleksandr and {van Buuren}, Stef and Jaddoe, Vincent W. V.},
  year = {2018},
