  journal = {Heredity (Edinb)},
  keywords = {Data Interpretation; Statistical,Genetic Predisposition to Disease,Genetic Testing,Genetics; Medical,Models; Genetic,Research Design},
  language = {eng},
  number = {3},
  pmid = {16077740}
}

@article{li2008,
