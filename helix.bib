}

@article{rosenquist2017,
  title = {Prenatal and Postnatal {{PCB}}-153 and p,p'-{{DDE}} Exposures and Behavior Scores at 5\textendash 9 Years of Age among Children in {{Greenland}} and {{Ukraine}}},
