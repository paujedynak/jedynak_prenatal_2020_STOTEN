  author = {Woskie, Susan and Kongtip, Pornpimol and Thanasanpaiboon, Wiroj and Kiatdamrong, Napaporn and Charoonrungsirikul, Nantha and Nankongnab, Noppanun and Surach, Anu and Phamonphon, Akkarat},
