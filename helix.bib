@article{mustieles2015,
  ids = {mustieles2015a},
  title = {Bisphenol {{A}}: {{Human}} Exposure and Neurobehavior},
  shorttitle = {Bisphenol {{A}}},
  author = {Mustieles, Vicente and {P{\'e}rez-Lobato}, Roc{\'i}o and Olea, Nicol{\'a}s and Fern{\'a}ndez, Mariana F.},
