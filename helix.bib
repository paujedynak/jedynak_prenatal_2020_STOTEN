  file = {C\:\\Users\\pauli\\Zotero\\storage\\V6FNTK5E\\Yang - 2004 - A simple method for estimating global DNA methylat.pdf},
  journal = {Nucleic Acids Res},
  language = {en},
  number = {3}
}

