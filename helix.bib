}

@article{migliore2015,
  title = {Prenatal {{Paracetamol Exposure}} and {{Wheezing}} in {{Childhood}}: {{Causation}} or {{Confounding}}?},
  shorttitle = {Prenatal {{Paracetamol Exposure}} and {{Wheezing}} in {{Childhood}}},
  author = {Migliore, Enrica and Zugna, Daniela and Galassi, Claudia and Merletti, Franco and Gagliardi, Luigi and Rasero, Laura and Trevisan, Morena and Rusconi, Franca and Richiardi, Lorenzo},
