@article{lim_prenatal_2017,
  title = {Prenatal and Postnatal Bisphenol {{A}} Exposure and Social Impairment in 4-Year-Old Children},
  author = {Lim, Youn-Hee and Bae, Sanghyuk and Kim, Bung-Nyun and Shin, Choong Ho and Lee, Young Ah and Kim, Johanna Inhyang and Hong, Yun-Chul},
  year = {2017},
  month = jul,
  volume = {16},
