  file = {C\:\\Users\\pauli\\Zotero\\storage\\LGP2AXVN\\Korecka et al. - 2017 - Repulsive Guidance Molecule a (RGMa) Induces Neuro.pdf;C\:\\Users\\pauli\\Zotero\\storage\\PTFKLLBY\\9361.html},
  journal = {J. Neurosci.},
  keywords = {AAV-mediated overexpression,dopamine neuron degeneration,Parkinson's disease,RGMa},
