  shorttitle = {{{LINE}}-1 Gene Hypomethylation and P16 Gene Hypermethylation in {{HepG2}} Cells Induced by Low-Dose and Long-Term Triclosan Exposure},
  author = {Zeng, Liudan and Ma, Huimin and Pan, Shangxia and You, Jing and Zhang, Gan and Yu, Zhiqing and Sheng, Guoying and Fu, Jiamo},
