  keywords = {Alleles,Animals,Brain,DNA Methylation,Female,Genome-Wide Association Study,Genomic Imprinting,Haplotypes,Humans,Immune System Diseases,Macaca mulatta,Macaca radiata,Nervous System Diseases,Placenta,Polymorphism; Single Nucleotide,Pregnancy,Quantitative Trait Loci,Species Specificity,T-Lymphocytes,Trans-Activators},
