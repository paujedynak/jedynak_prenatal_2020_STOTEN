@article{almeida2018,
  title = {Bisphenol {{A}}: {{Food Exposure}} and {{Impact}} on {{Human Health}}},
  shorttitle = {Bisphenol {{A}}},
  author = {Almeida, Susana and Raposo, Ant{\'o}nio and Almeida-Gonz{\'a}lez, Maira and Carrascosa, Conrado},
