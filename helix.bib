  pmid = {26996156}
}

@article{puzhko2011,
  title = {Parathyroid {{Hormone Signaling}} via {{G$\alpha$s Is Selectively Inhibited}} by an {{NH2}}-{{Terminally Truncated G$\alpha$s}}: {{Implications}} for {{Pseudohypoparathyroidism}}},
  shorttitle = {Parathyroid {{Hormone Signaling}} via {{G$\alpha$s Is Selectively Inhibited}} by an {{NH2}}-{{Terminally Truncated G$\alpha$s}}},
  author = {Puzhko, Svetlana and Goodyer, Cynthia Gates and Kerachian, Mohammad Amin and Canaff, Lucie and Misra, Madhusmita and J{\"u}ppner, Harald and Bastepe, Murat and Hendy, Geoffrey N},
