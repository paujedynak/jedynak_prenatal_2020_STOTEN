}

@article{ocallaghan2020,
  title = {Modulation of Placental Gene Expression in Small-for-Gestational-Age Infants},
  author = {O'Callaghan, Jessica L. and Clifton, Vicki L. and Prentis, Peter and Ewing, Adam and Miller, Yvette D. and Pelzer, Elise S.},
  year = {2020},
